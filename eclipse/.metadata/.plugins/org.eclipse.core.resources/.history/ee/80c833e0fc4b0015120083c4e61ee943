		world.setBlockState(new BlockPos(varX + 4, varY, varZ + 6), net.minecraft.init.Blocks.stonebrick.getDefaultState());
		world.setBlockState(new BlockPos(varX + 4, varY, varZ + 7), net.minecraft.init.Blocks.stone_slab.getStateFromMeta(5));
		world.setBlockState(new BlockPos(varX + 4, varY, varZ + 9), net.minecraft.init.Blocks.stonebrick.getDefaultState());
		world.setBlockState(new BlockPos(varX + 4, varY, varZ + 10), net.minecraft.init.Blocks.stonebrick.getDefaultState());
		
		world.setBlockState(new BlockPos(varX + 5, varY, varZ + 3), net.minecraft.init.Blocks.stone_slab.getStateFromMeta(5));
