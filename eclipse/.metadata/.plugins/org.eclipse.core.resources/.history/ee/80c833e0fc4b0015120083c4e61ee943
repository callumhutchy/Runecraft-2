		world.setBlockState(new BlockPos(varX + 4, varY, varZ + 10), net.minecraft.init.Blocks.stonebrick.getDefaultState());
		
		world.setBlockState(new BlockPos(varX + 6, varY, varZ), net.minecraft.init.Blocks.stonebrick.getDefaultState());
		world.setBlockState(new BlockPos(varX + 6, varY, varZ + 1), net.minecraft.init.Blocks.stonebrick.getDefaultState());
